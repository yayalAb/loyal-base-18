<?xml version="1.0" encoding="utf-8" ?>
<odoo>
   
    <record id="hospital_coverage_line_view_tree" model="ir.ui.view">
        <field name="name">hospital.coverage.line.view.tree</field>
        <field name="model">hospital.coverage.line</field>
        <field name="arch" type="xml">
            <list>
                <field name="product_categ_id"/>
                <field name="product_id"/>
                <field name="percentage" />
                <field name="amount"/>
            </list>
        </field>
    </record>
    <!--Lab test menu action-->
    <record id="hospital_coverage_line_action" model="ir.actions.act_window">
        <field name="name">hospital.coverage.line</field>
        <field name="res_model">hospital.coverage.line</field>
        <field name="view_mode">list</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Coverage
            </p>
        </field>
    </record>




        <record id="insurance_pricing_line_view_tree" model="ir.ui.view">
        <field name="name">insurance.pricing.line.view.tree</field>
        <field name="model">insurance.pricing.line</field>
        <field name="arch" type="xml">
            <list>
                 <field name="product_id"/>
                 <field name="amount"/>
            </list>
        </field>
    </record>
    <!--Lab test menu action-->
    <record id="insurance_pricing_line_action" model="ir.actions.act_window">
        <field name="name">insurance.pricing.line</field>
        <field name="res_model">insurance.pricing.line</field>
        <field name="view_mode">list</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Price
            </p>
        </field>
    </record>
  
</odoo>
