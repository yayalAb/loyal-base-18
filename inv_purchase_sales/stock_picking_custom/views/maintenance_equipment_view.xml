<odoo>
    <record id="maintenance_equipment_view_inherited_form" model="ir.ui.view">
        <field name="name">maintenance.equipment.view.inherited.form</field>
        <field name="model">maintenance.equipment</field>
        <field name="inherit_id" ref="maintenance.hr_equipment_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='cost']" position='after'>
                <field name='quantity' />
            </xpath>
        </field>
    </record>
</odoo>