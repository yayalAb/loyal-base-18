<?xml version="1.0" encoding="utf-8"?>
<odoo>
       <record  id="insurance_renewal_tree_view_id" model="ir.ui.view">
           <field name="name">insurance_id renewl history</field>
           <field name="model">insurance.renewal.history</field>
           <field name="arch" type="xml">
               <list>
                    <field name="insurance_id"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="create_date"/>
                    <field name="status"/>
                    <field name="total_annual_premium"/>
               </list>
           </field>
       </record>
    <record  id="insurance_renewal_form_view_id" model="ir.ui.view">
           <field name="name">insurance_id renewal history</field>
           <field name="model">insurance.renewal.history</field>
           <field name="arch" type="xml">
               <form>
                   <sheet>
                   <group>
                        <field name="insurance_id" readonly="1"/>
                       <group>
                           <field name="start_date"/>
                           <field name="total_annual_premium"/>
                       </group>
                       <group><field name="end_date"/></group>
                   </group>
                   </sheet>
                    <footer>
                    <button string="Save" name="renew_project" type="object" class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
               </form>
           </field>
       </record>
       <record id="insurance_renewal_history_action_id" model="ir.actions.act_window">
           <field name="name">insurance renewal history</field>
           <field name="type">ir.actions.act_window</field>
          <field name="res_model">insurance.renewal.history</field>
           <field name="view_mode">list,form</field>
           <field name="type">ir.actions.act_window</field>
       </record>
</odoo>